<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.spring.project.professor.dao.ProfesserDAO">

	<select id="myPage" parameterType="String" resultType="com.spring.project.professor.vo.Vo">
 	SELECT * FROM employees e JOIN accountinfo a ON e.usernumber = a.usernumber WHERE e.usernumber=#{userNumber}
 	</select>
 	
</mapper>